</head>
<body class="crm_body_bg">


    <nav class="sidebar">
        <div class="logo">
            <a class="large_logo" href="~/assets/Admin_Panel/index-2.html"><img src="~/assets/Admin_Panel/img/LogoF.png" style="width: 235px !important;"></a>
            <div class="sidebar_close_icon d-lg-none">
                <i class="ti-close"></i>
            </div>
        </div>
        <ul id="sidebar_menu">
            <li>
                <a asp-controller="Admin" asp-action="Index" aria-expanded="false">
                    <div class="nav_icon_small">
                        <img src="~/assets/Admin_Panel/img/menu-icon/dashboard.svg">
                    </div>
                    <div class="nav_title">
                        <span>Dashboard</span>
                    </div>
                </a>
            </li>

            <li>
                <h4 class="menu-text"><span>CUSTOM</span> <i class="fas fa-ellipsis-h"></i> </h4>
            </li>

            <li>
                <h4 class="menu-text"><span>Elements</span> <i class="fas fa-ellipsis-h"></i> </h4>
            </li>
            <li>
                <a class="has-arrow" aria-expanded="false">
                    <div class="nav_icon_small">
                        <img src="~/assets/Admin_Panel/img/menu-icon/4.svg">
                    </div>
                    <div class="nav_title">
                        <span>Forms</span>
                    </div>
                </a>
                <ul>
                    <li><a asp-controller="Admin" asp-action="Recipe">Recipe</a></li>

                </ul>
            </li>
            <li>
                <a class="has-arrow" aria-expanded="false">
                    <div class="nav_icon_small">
                        <img src="~/assets/Admin_Panel/img/menu-icon/11.svg">
                    </div>
                    <div class="nav_title">
                        <span>Table</span>
                    </div>
                </a>
                <ul>
                    <li><a asp-controller="Admin" asp-action="Display_User_Recipe">Display User Recipe</a></li>
                    <li><a asp-controller="Admin" asp-action="Approved_User_Recipe">Display Approved Recipe</a></li>
                    <li><a asp-controller="Admin" asp-action="Display_Recipe">Display Recipe</a></li>
                    <li><a asp-controller="Admin" asp-action="DisplayRegister">Display User</a></li>
                    <li><a asp-controller="Admin" asp-action="DisplayFeedback">Display Feedback</a></li>
                    <li><a asp-controller="Admin" asp-action="DisplayContact">Display Contact</a></li>
                    <li><a asp-controller="Admin" asp-action="Display_Order">Display Orders</a></li>
                    <li><a asp-controller="Admin" asp-action="Display_Approved_Order">Display Approved Orders</a></li>
                </ul>
            </li>
        </ul>
    </nav>


    <div class="">
        <main role="main" class="pb-3">
            <section class="main_content dashboard_part large_header_bg">

                <div class="container-fluid g-0">
                    <div class="row">
                        <div class="col-lg-12 p-0 ">
                            <div class="header_iner d-flex justify-content-between align-items-center">
                                <div class="sidebar_icon d-lg-none">
                                    <i class="ti-menu"></i>
                                </div>
                                <div class="form-label switch_toggle d-none d-lg-block" for="checkbox">
                                    <input type="checkbox" id="checkbox">
                                    <div class="slider round open_miniSide"></div>
                                </div>
                                <div class="header_right d-flex justify-content-between align-items-center">
                                    <div class="profile_info">
                                        <img src="~/assets/Admin_Panel/img/client_img.png">
                                        <div class="profile_info_iner">
                                            <div class="profile_author_name">
                                                @using System.Security.Claims;

                                                    @{
                                                        string userName = User.FindFirst(ClaimTypes.Name)?.Value;
                                                    }

                                                    @if (!string.IsNullOrEmpty(userName))
                                                    {
                                                    <h3>
                                                        <a asp-controller="Admin" asp-action="Index" class="text-dark">
                                                            @userName
                                                        </a>
                                                    </h3>
                                                    <h5><a style="color: white;" asp-area="" asp-controller="Admin" asp-action="Logout">Logout</a></h5>
                                                }
                                                else
                                                {
                                                    <li>
                                                        <a asp-area="" asp-controller="Admin" asp-action="LoginA">Login</a>
                                                    </li>
                                                }
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            @RenderBody()
            </section>
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="footer_part">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer_iner text-center">
                            <p>2020 © Influence - Designed by <a href="~/assets/Admin_Panel/#"> <i class="ti-heart"></i> </a><a href="~/assets/Admin_Panel/#"> DashboardPack</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </footer>
